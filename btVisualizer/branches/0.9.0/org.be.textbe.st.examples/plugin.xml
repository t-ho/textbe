<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.be.textbe.st.examples.category"
            name="%category.name"
            parentCategory="org.eclipse.ui.Examples">
      </category>
      <wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/org.be.textbe.st.examples.category"
            class="org.be.textbe.st.examples.OCLComparisonExampleWizard"
            hasPages="false"
            icon="$nl$/icons/beicon.gif"
            id="org.be.textbe.st.examples.ocl"
            name="%wizard.name"
            project="true">
         <description>
            %wizard.description
         </description>
      </wizard>
      <wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/org.be.textbe.st.examples.category"
            class="org.be.textbe.st.examples.STNotationExampleWizard"
            hasPages="false"
            icon="$nl$/icons/beicon.gif"
            id="org.be.textbe.st.examples.notation"
            name="%wizard.name.0"
            project="true">
         <description>
            %wizard.description.0
         </description>
      </wizard>
   </extension>

</plugin>
