<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.be.textbe.st.textst.resource.st.mopp.StMetaInformation" id="org.be.textbe.st.textst.resource.st.mopp.StMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="org.be.textbe.st.textst.resource.st.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="org.be.textbe.st.textst.resource.st.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.be.textbe.st.textst.resource.st.problem">
		</super>
	</extension>
	
	<extension id="org.be.textbe.st.textst.resource.st.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.be.textbe.st.textst.resource.st.problem">
		</super>
	</extension>
	
	<extension id="org.be.textbe.st.textst.resource.st.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.be.textbe.st.textst.resource.st.problem">
		</super>
	</extension>
	
	<extension id="org.be.textbe.st.textst.resource.st.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.be.textbe.st.textst.resource.st.problem">
		</super>
	</extension>
	
	<extension id="org.be.textbe.st.textst.resource.st.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.be.textbe.st.textst.resource.st.problem">
		</super>
	</extension>
	
	<extension id="org.be.textbe.st.textst.resource.st.nature" name="st nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.be.textbe.st.textst.resource.st.mopp.StNature">
			</run>
		</runtime>
		<builder id="org.be.textbe.st.textst.resource.st.builder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.be.textbe.st.textst.resource.st.builder" name="st Builder">
		<builder hasNature="true">
			<run class="org.be.textbe.st.textst.resource.st.mopp.StBuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension-point id="org.be.textbe.st.textst.resource.st.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.be.textbe.st.textst.resource.st.mopp.StResourceFactoryDelegator" type="st">
		</parser>
	</extension>
	
	<extension-point id="org.be.textbe.st.textst.resource.st.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd">
	</extension-point>
	
	<extension point="org.eclipse.debug.core.breakpoints">
		<breakpoint class="org.be.textbe.st.textst.resource.st.debug.StLineBreakpoint" id="org.be.textbe.st.textst.resource.st.debug.breakpoint" markerType="org.be.textbe.st.textst.resource.st.debug.lineBreakpoint.marker" name="st Breakpoint">
		</breakpoint>
	</extension>
	
	<extension id="org.be.textbe.st.textst.resource.st.debug.lineBreakpoint.marker" point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.debug.core.lineBreakpointMarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<persistent value="true">
		</persistent>
	</extension>
	
	<extension point="org.eclipse.debug.core.launchConfigurationTypes">
		<launchConfigurationType id="org.be.textbe.st.textst.resource.st.ui.launchConfigurationType" delegate="org.be.textbe.st.textst.resource.st.launch.StLaunchConfigurationDelegate" modes="run,debug" name="st Application" sourceLocatorId="org.be.textbe.st.textst.resource.st.ui.sourceLocator" sourcePathComputerId="org.be.textbe.st.textst.resource.st.ui.sourcePathComputer" delegateName="st Launch Tooling" delegateDescription="This will run or debug .st files.">
		</launchConfigurationType>
	</extension>
	
	<extension point="org.eclipse.debug.core.sourcePathComputers">
		<sourcePathComputer class="org.be.textbe.st.textst.resource.st.debug.StSourcePathComputerDelegate" id="org.be.textbe.st.textst.resource.st.ui.sourcePathComputer">
		</sourcePathComputer>
	</extension>
	
	<extension point="org.eclipse.debug.core.sourceLocators">
		<sourceLocator id="org.be.textbe.st.textst.resource.st.ui.sourceLocator" class="org.be.textbe.st.textst.resource.st.debug.StSourceLocator" name="st Source Locator">
		</sourceLocator>
	</extension>
	
</plugin>
